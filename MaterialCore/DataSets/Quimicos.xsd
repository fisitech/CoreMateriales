                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     MaterialesOC.Id, MaterialesOC.ClienteId, MaterialesOC.RenglonId, MaterialesOC.NumeroParte, MaterialesOC.Descripcion, 
                      MaterialesOC.PqOrd - MaterialesOC.PqRec AS CantidadAbierta, MaterialesOC.CantidadOriginal, MaterialesOC.PqOrd, MaterialesOC.PqRec, 
                      MaterialesOC.Precio, MaterialesOC.ProveedorID, MaterialesOC.Descripcion_P, MaterialesOC.FechaRecibo, ISNULL(MaterialesOC.CantidadRecibida, 0) 
                      AS CantidadRecibida, MaterialesOC.UnidadMedida, MaterialesOC.FacturaProveedor, MaterialesOC.Pais, MaterialesOC.Planta, 
                      MaterialesOC.Comentario, MaterialesOC.Bultos, MaterialesOC.NoGuia, MaterialesOC.Moneda, MaterialesOC.ProgramaId, MaterialesOC.DrawingId, 
                      MaterialesOC.NombreProveedor, MaterialesOC.Text1, ISNULL(sku_escanear.escaneado, 0) AS escaneado, 
                      CASE WHEN MaterialesITAR.NumeroParte IS NULL THEN 0 ELSE 1 END AS ITAR, CASE WHEN MaterialesQuimicos.NumeroParte IS NULL 
                      THEN 0 ELSE 1 END AS Quimico, ISNULL(sku_arrivals.dfecha_sku, MaterialesOC.FechaRecibo) AS fecha_sku
FROM         MaterialesOC LEFT OUTER JOIN
                      sku_arrivals ON MaterialesOC.NoGuia = sku_arrivals.nid_sku LEFT OUTER JOIN
                      MaterialesITAR ON MaterialesOC.NumeroParte = MaterialesITAR.NumeroParte LEFT OUTER JOIN
                      MaterialesQuimicos ON MaterialesOC.NumeroParte = MaterialesQuimicos.NumeroParte LEFT OUTER JOIN
                      sku_escanear ON MaterialesOC.NoGuia = sku_escanear.nid_sku
WHERE     (MaterialesOC.Id = @Id) AND (ISNULL(MaterialesOC.CantidadRecibida, 0) &lt; MaterialesOC.PqOrd - MaterialesOC.PqRec) AND (@cerradas = 0) OR
                      (MaterialesOC.Id = @Id) AND (@cerradas = 1)
ORDER BY MaterialesOC.RenglonId</CommandText>
